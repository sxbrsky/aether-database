{
    "name": "sxbrsky/zinc",
    "description": "A high-performance PHP database abstract layer with Object-Oriented API and more many features.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "database",
        "mysql",
        "mariadb",
        "pgsql",
        "postgresql",
        "querybuilder",
        "pdo",
        "sql",
        "sqlite"
    ],
    "support": {
        "issues": "https://github.com/sxbrsky/zinc/issues",
        "source": "https://github.com/sxbrsky/zinc"
    },
    "require": {
        "php": ">=8.2"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "dev-master",
        "jetbrains/phpstorm-attributes": "dev-master",
        "phpstan/phpstan": "2.0.x-dev",
        "phpunit/phpunit": "^11.4.x-dev"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Zinc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zinc\\Tests\\Unit\\": "tests/unit/",
            "Zinc\\Tests\\Stubs\\": "tests/stubs/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.19"
        },
        "sort-packages": true
    }
}
